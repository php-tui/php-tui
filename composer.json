{
    "name": "dantleech/php-tui",
    "description": "PHP TUI",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "DTL\\PhpTui\\": "src/",
            "DTL\\Cassowary\\": "lib/cassowary/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DTL\\PhpTui\\Tests\\": "tests/",
            "DTL\\Cassowary\\Tests\\": "lib/cassowary/tests/"
        }
    },
    "authors": [
        {
            "name": "Daniel Leech"
        }
    ],
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.34",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.4",
        "symfony/var-dumper": "^6.3"
    },
    "require": {
        "symfony/console": "^6.3"
    }
}
